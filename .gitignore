# Docker development environment .gitignore

# Generated docker-compose file (built from snippets during installation)
docker/docker-compose.yml

# Data directories (contain project files and databases)
data/

# Runtime files and logs
*.log
logs/

# User-specific configuration
.config/
.local/

# Site configurations generated during setup
docker/nginx/sites-enabled/
docker/apache/sites-enabled/

# Project-specific configurations in site directories
**/.siteconfig/

# Database data directories
docker/data/
data/mysql*/
data/mongo*/
data/elasticsearch*/
data/opensearch*/

# Temporary files
*.tmp
*.temp
temp/
tmp/

# OS specific files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# IDE files
.vscode/
.idea/
*.swp
*.swo
*~

# Backup files
*.bak
*.backup

# Docker volumes and bind mounts data
volumes/

# User home directories for PHP containers
data/home/*/

# SSL certificates (auto-generated)
*.pem
*.crt
*.key